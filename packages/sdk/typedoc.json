{
  // Comments are supported, like tsconfig.json
  "entryPoints": ["./src/index.ts"],
  "exclude": ["./test", "./src/**/*.test.ts"],
  "out": "docs",
  "plugin": ["typedoc-material-theme"],
  "themeColor": "#43E661",
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Class",
    "Interface",
    "Enum",
    "EnumMember",
    "TypeAlias",
    "Constructor",
    "Property",
    "Function",
    "Variable",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ]
}
